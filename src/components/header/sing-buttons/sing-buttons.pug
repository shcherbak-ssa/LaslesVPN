
mixin sing-buttons()
  .sing-buttons(class=attributes.class)
    +base-header-level-3('OR')(class="sing-buttons--header")
    .sing-buttons--container(data-class="flex")
      .sing-buttons--singin(
        data-class="flex-center click font-medium",
        data-button="sing-in"
      ) Sing In
      +base-button-small('Sing Up', 'sing-up')
